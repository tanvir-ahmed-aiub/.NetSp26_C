@model List<Student>
@{
    Layout = "_MyLayout";
}

<h1>List</h1>
@* <ol>
    @for (int i = 0; i < ViewBag.Names.Length; i++)
    {
        <li>@ViewBag.Names[i]</li>
    }
</ol> *@

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Cgpa</th>
            <th>HSCGPa</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Cgpa</td>
                <td>@item.HSCGpa</td>
            </tr>
        }
    </tbody>
</table>

